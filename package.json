{
  "name": "react-vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "npm run build && vite preview --port 3000",
    "prettier:check": "prettier --ignore-unknown --check src",
    "prettier:fix": "prettier --ignore-unknown --write src",
    "stylelint:css:check": "stylelint \"src/**/*.{css,scss,sass,styl,stylus}\"",
    "stylelint:css:fix": "npm run stylelint:css:check -- --fix",
    "stylelint:js:check": "stylelint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:check": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "test:e2e": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test serve http://127.0.0.1:3000/ 'cypress run'",
    "test:unit": "jest --watch",
    "test:unit:ci": "jest --passWithNoTests",
    "test:unit:coverage": "jest --silent --watchAll=false --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^3.1.0",
    "@emotion/babel-plugin": "^11.10.0",
    "@emotion/jest": "^11.10.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/testing-library__user-event": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.1",
    "cypress": "^10.4.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-testing-library": "^5.6.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "postcss-sass": "^0.5.0",
    "postcss-scss": "^4.0.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.10.0",
    "stylelint-config-clean-order": "^0.9.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-hudochenkov": "^8.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-stylus": "^0.16.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.6",
    "vite-plugin-eslint": "^1.8.0",
    "vite-plugin-svgr": "^2.2.1"
  }
}
